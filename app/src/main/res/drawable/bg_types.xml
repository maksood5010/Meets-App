<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/itemStroke">
        <shape android:shape="rectangle">
            <solid android:color="#B8B8B8" />
            <corners android:radius="@dimen/size30" />
        </shape>
    </item>
    <item
        android:id="@+id/itemSolid"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#D8D5D8" />
            <corners android:radius="@dimen/size30" />
        </shape>
    </item>
    <item
        android:id="@+id/itemDash"
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1.5dp"
                android:color="#F7F7F7"
                android:dashWidth="10dp"
                android:dashGap="5dp" />
            <corners android:radius="@dimen/size30" />
        </shape>
    </item>

</layer-list>
