<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/ivCheck"
        android:layout_width="20dp"
        android:layout_height="@dimen/size20"
        android:src="@drawable/ic_checked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/placename"
        app:layout_constraintBottom_toBottomOf="@+id/placename"/>

    <TextView
        android:id="@+id/placename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shisheo Lounge"
        android:textSize="@dimen/size14"
        android:fontFamily="@font/poppins_semibold"
        android:textColor="@color/blacktextColor"
        android:paddingStart="@dimen/size5"
        app:layout_constraintStart_toEndOf="@+id/ivCheck"
        app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>